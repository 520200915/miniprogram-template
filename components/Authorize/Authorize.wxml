<view class='open-seting-bg' wx:if='{{openSet}}' catchtap='cancel' catchtouchmove='noScroll'>
  <view class='open-set-inner'>
    <view class='set-title'>是否打开授权设置？</view>
    <view class='btn-openset'>
      <view catchtap='cancel' class='btn'>取消</view>
      <view class='btn'>
        确定
        <button open-type='{{buttonType}}' class='button-style ' catchtap='cancel'></button>
      </view>
    </view>
  </view>
</view>